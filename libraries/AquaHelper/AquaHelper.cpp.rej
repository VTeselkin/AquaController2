+String AquaHelper::GetFormatDataNow(tmElements_t tm){
+	String data_fm = "";
+		if (tm.Day < 10)
+			data_fm += "0";
+		data_fm += String(tm.Day) + "/";
+		if (tm.Month < 10)
+			data_fm += "0";
+		data_fm += String(tm.Month);
+		data_fm += "/";
+		data_fm += String(y2kYearToTm(tmYearToCalendar(tm.Year)));
+		return data_fm;
+}
